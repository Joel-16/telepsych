{
  "name": "Telepsych",
  "version": "1.0.0",
  "description": "Platform for sessions between patients and pschiatrists",
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "node ./dist/index.js",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc --noEmit",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown ."
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.6",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.33",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-array-reduce": "^1.0.58",
    "eslint-plugin-prettier": "^4.0.0",
    "ts-node-dev": "^1.1.8",
    "tsc-files": "^1.1.3",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": "~16.16.0",
    "npm": "~  8.11.0"
  },
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joel-16/payments.git"
  },
  "author": "Inaku Joel",
  "license": "ISC"
}
